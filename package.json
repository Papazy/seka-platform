    "mysql2": "^3.14.1",
    "next": "15.3.4",
    "next-auth": "5.0.0-beta.29",
    "prisma": "^6.10.1",
    "react": "^19.0.0",
    "react-ace": "^14.0.1",
    "react-dom": "^19.0.0",
